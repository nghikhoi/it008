<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:local="clr-namespace:UI"
                    xmlns:uc="clr-namespace:UI.Components">
    <DataTemplate x:Key="AddFriendNotification">
        <uc:FriendRequestNoti SenderName="{Binding SenderName}"/>
    </DataTemplate>
    <DataTemplate x:Key="FriendAccepectNotification">
        <uc:NotificationTagItem Content="{Binding Content}"/>
    </DataTemplate>
</ResourceDictionary>